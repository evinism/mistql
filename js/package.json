    "mocha": "^8.0.0",
    "rollup": "^2.56.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {}
